log4j.rootLogger=INFO, stdout , rf
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=\u6d41\u6c34\u53f7\uff1a%X{seqId} - \u4f1a\u8bddID\uff1a%X{sessionId} - %d{yyyy-MM-dd HH:mm:ss} %p - %l%m%n

log4j.appender.rf=org.apache.log4j.DailyRollingFileAppender
log4j.appender.rf.File=${catalina.home}/logs/tribe_touch.log
log4j.appender.rf.DatePattern='-'yyyy-MM-dd'.log'
log4j.appender.rf.layout=org.apache.log4j.PatternLayout
log4j.appender.rf.layout.ConversionPattern=\u6d41\u6c34\u53f7\uff1a%X{seqId} - \u4f1a\u8bddID\uff1a%X{sessionId} - %d{yyyy-MM-dd HH:mm:ss} %p - %l%m%n
